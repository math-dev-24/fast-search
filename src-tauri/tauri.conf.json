{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "fast-search",
  "version": "0.1.0",
  "identifier": "com.fast-search.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "fast-search",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/search-48.png",
      "icons/search-96.png",
      "icons/search-144.png",
      "icons/logo.ico"
    ]
  }
}
